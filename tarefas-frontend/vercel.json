{
  "version": 2,
  "builds": [
    {
      "src": "tarefas-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "tarefas-frontend/dist/tarefas-frontend/browser"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "./vercel-build.sh"
}
